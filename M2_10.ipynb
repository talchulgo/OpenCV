   "source": [
    "import cv2\n",
    "\n",
    "src = cv2.imread(\"images/rose.jpg\",cv2.IMREAD_GRAYSCALE)\n",
    "\n",
    "dst1 = cv2.GaussianBlur(src,(0,0),1)\n",
    "dst2 = cv2.GaussianBlur(src,(0,0),3)\n",
    "\n",
    "cv2.imshow(\"src\",src)\n",
    "cv2.imshow(\"dst1\",dst1)\n",
    "cv2.imshow(\"dst2\",dst2)\n",
    "\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  }
