    "import cv2\n",
    "\n",
    "img = cv2.imread(\"../images/rose.jpg\", cv2.IMREAD_GRAYSCALE)\n",
    "img1 = cv2.add(img,100)\n",
    "img2 = cv2.add(img, -50)\n",
    "\n",
    "cv2.imshow(\"img\",img)\n",
    "cv2.imshow(\"img1\",img1)\n",
    "cv2.imshow(\"img2\",img2)\n",
    "\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  }
